+	echo ' cd $(top_srcdir) && $(AUTOMAKE) --foreign test/sai_thrift_src/Makefile'; \
+	$(am__cd) $(top_srcdir) && \
+	  $(AUTOMAKE) --foreign test/sai_thrift_src/Makefile
+Makefile: $(srcdir)/Makefile.in $(top_builddir)/config.status
